# PuppyFlow (Next.js) environment examples - do not put real secrets here
DEPLOYMENT_MODE=local
USER_SYSTEM_BACKEND=http://localhost:8000
PUPPYENGINE_URL=http://localhost:8001
PUPPYSTORAGE_URL=http://localhost:8002
API_SERVER_URL=http://localhost:8004
SERVICE_KEY=
ALLOW_VERIFY_WITHOUT_SERVICE_KEY=false
USER_SYSTEM_FRONTEND_URL=http://localhost:3000
