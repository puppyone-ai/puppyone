{
  "include": [
    "src",
    "mcp_service"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.ruff_cache",
    "**/.venv",
    "**/venv",
    "**/node_modules",
    "**/.git",
    ".claude",
    ".cursor",
    "docs",
    "openspec",
    "example",
    "sql",
    "script",
    "tests"
  ],
  "ignore": [
    "**/test_*.py"
  ],
  "pythonVersion": "3.12",
  "pythonPlatform": "Darwin",
  "typeCheckingMode": "off",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}
